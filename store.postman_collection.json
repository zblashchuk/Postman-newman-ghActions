						{
							"listen": "test",
							"script": {
								"exec": [
									"eval(globals.testsResponseTimeSuccessCode)();\r",
									"pm.test(\"Body matches response necessary keys\", function () {\r",
									"    pm.response.to.have.jsonBody('name')\r",
									"    .and.to.have.jsonBody('price')\r",
									"    .and.to.have.jsonBody('sku')\r",
									"    .and.to.have.jsonBody('description')\r",
									"    ;\r",
									"});\r",
									"pm.test(\"Content-Type header is present\", () => {\r",
									"  pm.response.to.have.header(\"Content-Type\");\r",
									"});\r",
									"pm.test(\"Content-Type header is application/json\", () => {\r",
									"  pm.expect(pm.response.headers.get('Content-Type')).to.eql('application/json; charset=utf-8');\r",
									"});\r",
									"pm.test(\"Create correct product\", () => {\r",
									"  const responseJson = pm.response.json();\r",
									"  pm.expect(responseJson.name).to.eql(\"orange\");\r",
									"  pm.expect(responseJson.price).to.eql(8);\r",
									"  pm.expect(responseJson.description).to.eql(\"wouh\");\r",
									"});\r",
									"const jsonData = pm.response.json();\r",
									"pm.test(\"Test data type of the response\", () => {\r",
									"  pm.expect(jsonData).to.be.an(\"object\");\r",
									"  pm.expect(jsonData.name).to.be.a(\"string\");\r",
									"  pm.expect(jsonData.price).to.be.a(\"number\");\r",
									"});\r",
									"pm.environment.set(\"id_product\", pm.response.json().id);\r",
									"\r",
									"\r",
									""
								],
								"type": "text/javascript"
							}
